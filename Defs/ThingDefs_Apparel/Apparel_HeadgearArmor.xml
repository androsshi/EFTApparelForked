  <ThingDef Name="EFTHelmetC1Base" ParentName="ArmorHelmetMakeableBase">
    <defName>JDSTarkov_Headgear_Soft_Tank_Crew</defName>
    <label>Soft Tank Crew</label>
    <description>Tank headset TSh-4M-L (Z) -01 is designed to provide two-way radiotelephone communication in objects with a high level of noise, to protect it from climatic factors and from impacts on structural elements of objects.</description>
    <graphicData>
      <texPath>Things/Headgear/Soft_Tank_Crew/Soft_Tank_Crew_UI</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <thingCategories Inherit="False">
      <li>EFTHeadgearArmorC1</li>
    </thingCategories>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
    </recipeMaker>
    <costList>
      <Cloth>10</Cloth>
      <Steel>25</Steel>
    </costList>
    <statBases>
      <Mass>1.2</Mass>
      <Bulk>4</Bulk>
      <WornBulk>1</WornBulk>
      <WorkToMake>3500</WorkToMake>
      <MaxHitPoints>50</MaxHitPoints>
      <ArmorRating_Sharp>0.12</ArmorRating_Sharp>
      <ArmorRating_Blunt>2</ArmorRating_Blunt>
      <ArmorRating_Heat>0.2</ArmorRating_Heat>
      <Insulation_Cold>0.50</Insulation_Cold>
      <Insulation_Heat>0.02</Insulation_Heat>
      <EquipDelay>1</EquipDelay>
    </statBases>
    <apparel>
      <wornGraphicPath>Things/Headgear/Soft_Tank_Crew/Soft_Tank_Crew</wornGraphicPath>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>IndustrialBasic</li>
        <li>IndustrialMilitaryBasic</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <useDeflectMetalEffect>true</useDeflectMetalEffect>
    </apparel>
  </ThingDef>

