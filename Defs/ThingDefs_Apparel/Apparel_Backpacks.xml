  <!-- == Tactical Sling Bag == -->
  <ThingDef Name="EFTSmallBase" ParentName="ApparelCarryGearBase">
    <defName>JDSTarkov_Backpack_TacticalSling</defName>
    <label>Tactical Sling Bag</label>
    <description>An original sling bag for everyday wear or for active rest.</description>
    <graphicData>
      <texPath>Things/Backpack/Tactical_Sling_Bag/Tactical_Sling_Bag_UI</texPath>
      <graphicClass>Graphic_Single</graphicClass> 	  
    </graphicData>
    <thingCategories>
      <li>EFTBackpacksSmall</li>
    </thingCategories>
    <costList>
      <Cloth>25</Cloth>
    </costList>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <WorkToMake>4000</WorkToMake>
      <Mass>1</Mass>
      <Bulk>3</Bulk>
      <WornBulk>1</WornBulk> 
    </statBases>
    <equippedStatOffsets>
      <CarryBulk>20</CarryBulk>
    </equippedStatOffsets>
    <apparel>
      <wornGraphicPath>Things/Backpack/Tactical_Sling_Bag/Tactical_Sling_Bag</wornGraphicPath>
      <wornGraphicData>
        <renderUtilityAsPack>true</renderUtilityAsPack>
        <east>
          <offset>(-0.35,-0.05)</offset>
          <thin><offset>(0.05,0)</offset></thin>
          <hulk><offset>(-0.1,0)</offset></hulk>
          <fat><offset>(-0.15,0)</offset></fat>
		      <female><offset>(0.02,0)</offset></female>
        </east>
        <west>
          <offset>(0.35,-0.05)</offset>
          <thin><offset>(-0.05,0)</offset></thin>
          <hulk><offset>(0.1,0)</offset></hulk>
          <fat><offset>(0.05,0)</offset></fat>
		      <female><offset>(-0.02,0)</offset></female>		  
        </west>
        <north>
          <offset>(0,-0.1)</offset>
          <thin><offset>(0,0)</offset></thin>
          <hulk><offset>(0,-0.05)</offset></hulk>
          <fat><offset>(0,-0.05)</offset></fat>
        </north>		
        <male><scale>(0.90,0.90)</scale></male> 
        <thin><scale>(0.85,0.85)</scale></thin>
        <hulk><scale>(1.10,1.10)</scale></hulk>
        <fat><scale>(1.10,1.10)</scale></fat>
        <female><scale>(0.90,0.90)</scale></female> 
      </wornGraphicData>
